#!/bin/sh
if [ -f ~/tmp/Dream_Aquarium_-_2_Hours_-_8_Tanks_4K.mkv ]; then
    youtube-dl -f 401 "https://www.youtube.com/watch?v=HHi8qOtHnhE"
fi
killall swaybg
mpvpaper-holder -p -s -o "--hwdec=auto --gpu-context=wayland no-audio" $(swaymsg -t get_outputs | jq -r '.[] | select(.focused)' | jq -r '.name') ~/tmp/Dream_Aquarium_-_2_Hours_-_8_Tanks_4K.mkv
