message/rfc822; formail -b < %s > %s.mbox && mutt -f '%s.mbox'; needsterminal;
text/plain; kak %s;
text/html; qutebrowser --desktop-file-name qutepreview --temp-basedir -C ~/.config/qutebrowser/qutepreview.py %s; needsterminal; nametemplate=%s.html;
text/html; w3m -I %{charset} -T text/html; copiousoutput;
application/pdf; zathura %s;
application/pdf; pdftotext -l 10 -nopgbrk -q -- %s - | fmt -w 120; copiousoutput;
image/jpeg; vimiv %s;
image/png; vimiv %s;
application/ics; mutt-ics; copiousoutput;
text/calendar; mutt-ics; copiousoutput;
