[calendars]

[[personal]]
path = ~/library/calendars/personal/
color = dark green

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
local_timezone= Europe/Paris
default_timezone= Europe/Paris

[highlight_days]
color = light blue

[keybindings]
external_edit = E
save = w

[default]
highlight_event_days = True
default_calendar = personal
print_new = event
show_all_days = True
timedelta = 1d
