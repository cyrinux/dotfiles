[calendars]

[[personal]]
path = ~/.calendars/personal/
color = dark green

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M
local_timezone= Europe/Paris
default_timezone= Europe/Paris

[highlight_days]
color = light blue

[keybindings]
external_edit = E
save = w
